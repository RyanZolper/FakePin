

<h1><%= link_to("FakePin", pins_path, {:style=>'color:#FF0000;', :class => "css_class"}) %></h1>
<%= link_to 'My Pins', mypins_path %>
<%= link_to 'New Pin', new_pin_path(@user) %>
<%= link_to 'My Boards', myboards_path %>
<%= link_to 'Log Out', users_logout_path %>

<button type="button" class="btn btn-primary btn-sm" data-toggle="modal" data-target="#myModal">
  Click for Pointless Popup
</button>

<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Yadadadadad</h4>
      </div>
      <div class="modal-body">
        asldjkfhaoisdjhfaoidsjfhaosdifjh
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>

<h4>
<%= form_tag(pins_path, :method => "get", class: "navbar-form", id: "search-form") do %>
    <div class="input-append">
      <%= text_field_tag :search, params[:search], class: "span2", placeholder: "Search Pins" %>
      <button class="btn btn-primary" type="submit"><i class="icon-search"></i>Search</button>
    </div>
  <% end %>
</h4>



<div class="container">
  <div class="row">
    <% @pins.each do |pin| %>
      <div class="item">
        <div class="well">
          <%= link_to pin do %>
          <%= image_tag("#{pin.pinimage}", class: "img-responsive img-rounded") %>
          <% end %>


          <%= link_to pinit_path(pin), 'data-popup' => true, 'data-pin-config' => "above" do %>
            <%= image_tag("//assets.pinterest.com/images/pidgets/pin_it_button.png") %>

          <% end %>

          <br>
          <h2><%= pin.title %></h2>
          <h3><%= pin.description %></h3>
          <h3><%= pin.user.name %></h3>
        </div>

      </div>


    <% end %>
  </div>
</div>


